{
  "expo": {
    "name": "Taaza",
    "slug": "taaza",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/taaza.png",
    "scheme": "taaza",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/taaza.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b70e24d2-6d9f-4116-b835-d7a746f96835"
      }
    },
    "android": {
      "package": "com.taaza.app"
    },
    "splash": {
      "image": "./assets/images/taaza.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    }
  }
}
